# ============================================
# PRIMEIROCV - VARIÁVEIS DE AMBIENTE
# ============================================
# Copie este arquivo para .env.local e preencha os valores
# NUNCA commite o arquivo .env.local

# ============================================
# FIREBASE
# ============================================
# Obtenha essas credenciais no Console do Firebase:
# https://console.firebase.google.com
# Projeto > Configurações > Geral > Seus apps > SDK Firebase

VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# ============================================
# AMBIENTE
# ============================================
VITE_APP_ENV=development
VITE_APP_URL=http://localhost:3000

# ============================================
# EMULADORES (opcional, para desenvolvimento)
# ============================================
# Defina como 'true' para usar emuladores locais do Firebase
VITE_USE_EMULATORS=false
